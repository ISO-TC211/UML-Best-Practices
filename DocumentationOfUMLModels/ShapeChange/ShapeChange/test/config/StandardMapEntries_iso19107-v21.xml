<?xml version="1.0" encoding="UTF-8"?>
<xsdMapEntries xmlns="http://www.interactive-instruments.de/ShapeChange/Configuration/1.1">
	<!-- ISO 19107 Geometry -->
	<XsdMapEntry type="GM_Object" xsdEncodingRules="iso19136_2007 gml33" xmlType="gml:AbstractGeometryType" xmlElement="gml:_Geometry" xmlPropertyType="gml:GeometryPropertyType" xmlTypeNilReason="false"/>
	<XsdMapEntry type="GM_Point" xsdEncodingRules="iso19136_2007 gml33" xmlType="gml:PointType" xmlElement="gml:Point" xmlPropertyType="gml:PointPropertyType" xmlTypeNilReason="false"/>
	<XsdMapEntry type="GM_LineString" xsdEncodingRules="iso19136_2007 gml33" xmlElement="gml:LineString" xmlType="gml:LineStringType" xmlPropertyType="gml:LineStringPropertyType" xmlTypeNilReason="false"/>
	<XsdMapEntry type="GM_Polygon" xsdEncodingRules="iso19136_2007 gml33" xmlElement="gml:Polygon" xmlType="gml:PolygonType" xmlPropertyType="gml:PolygonPropertyType" xmlTypeNilReason="false"/>
	<XsdMapEntry type="GM_MultiPoint" xsdEncodingRules="iso19136_2007 gml33" xmlType="gml:MultiPointType" xmlElement="gml:MultiPoint" xmlPropertyType="gml:MultiPointPropertyType" xmlTypeNilReason="false"/>
	<XsdMapEntry type="GM_MultiLineString" xsdEncodingRules="iso19136_2007 gml33" xmlElement="gml:MultiLineString" xmlType="gml:MultiLineStringType" xmlPropertyType="gml:MultiLineStringPropertyType" xmlTypeNilReason="false"/>
	<XsdMapEntry type="GM_MultiPolygon" xsdEncodingRules="iso19136_2007 gml33" xmlElement="gml:MultiPolygon" xmlType="gml:MultiPolygonType" xmlPropertyType="gml:MultiPolygonPropertyType" xmlTypeNilReason="false"/>
	<!-- GML 2.1 only provides a limited set of realizations for the following GM types. 	
		 gml:LineString represents an implementation of GM_Curve, where only GM_LineStrings are used as GM_CurveSegments (of which a GM_Curve consists).
		 gml:Polygon represents an implementation of GM_Surface, where a single GM_Polygon is used as GM_SurfacePatch (of which a GM_Surface consists).
		 See ISO 19136:2007 clauses D.2.3.4, D.3.5 and D.3.6. -->
	<XsdMapEntry type="GM_Curve" xsdEncodingRules="iso19136_2007 gml33" xmlElement="gml:LineString" xmlType="gml:LineStringType" xmlPropertyType="gml:LineStringPropertyType" xmlTypeNilReason="false"/>
	<XsdMapEntry type="GM_MultiCurve" xsdEncodingRules="iso19136_2007 gml33" xmlElement="gml:MultiLineString" xmlType="gml:MultiLineStringType" xmlPropertyType="gml:MultiLineStringPropertyType" xmlTypeNilReason="false"/>	
	<XsdMapEntry type="GM_Surface" xsdEncodingRules="iso19136_2007 gml33" xmlElement="gml:Polygon" xmlType="gml:PolygonType" xmlPropertyType="gml:PolygonPropertyType" xmlTypeNilReason="false"/>
	<XsdMapEntry type="GM_MultiSurface" xsdEncodingRules="iso19136_2007 gml33" xmlElement="gml:MultiPolygon" xmlType="gml:MultiPolygonType" xmlPropertyType="gml:MultiPolygonPropertyType" xmlTypeNilReason="false"/>
	<!-- 3D geometry types are not supported by GML 2.1 -->
	<XsdMapEntry type="GM_Solid" xsdEncodingRules="iso19136_2007 gml33" xmlPropertyType="string" xmlType="string" xmlTypeType="simple" xmlTypeContent="simple"/>
	<XsdMapEntry type="GM_MultiSolid" xsdEncodingRules="iso19136_2007 gml33" xmlPropertyType="string" xmlType="string" xmlTypeType="simple" xmlTypeContent="simple"/>
	
	<!-- ... more to be added as required -->
</xsdMapEntries>
